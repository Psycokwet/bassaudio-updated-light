  "license": "MIT",
  "author": "Sophie Carboni <socarboni@gmail.com>",
  "description": "un4seen Bass Audio Library based on ffi bindings, tested and updated",
  "main": "./package/bass",
  "scripts": {
    "gitacp": "npm run ecf && npm run ecs && npm run ecc && node tools/g.js",
    "ecf": "node tools/extract-covered-fun.js > documentation/covered-fun.md",
    "ecs": "node tools/extract-covered-struct.js > documentation/covered-structs.md",
    "ecc": "node tools/extract-covered-callback.js > documentation/covered-callbacks.md",
    "localTest": "jest",
    "test": "jest --coverage > report.txt && node tools/extract-coverage.js > coverage.txt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Psycokwet/bassaudio-updated-light"
  },
  "keywords": [
    "bass",
    "audio",
    "un4seen",
    "mp3",
    "icecast",
    "shoutcast",
    "shoutcast2",
    "stream",
    "encoder",
    "audio encoder",
    "audio mixer",
    "mixer",
    "media player",
    "cross platform media player",
    "bass library"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "ffi-napi": "^3.1.0",
    "ref-array-di": "^1.2.2",
    "ref-napi": "^3.0.1",
    "ref-struct-di": "^1.1.1",
    "replace-in-file": "^6.1.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "os": "^0.1.1"
  }
}
